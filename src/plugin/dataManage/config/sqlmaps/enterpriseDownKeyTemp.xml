<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="enterpriseDownKeyTemp">
	<typeAlias alias="EnterpriseDownKeyTemp" type="com.tianque.plugin.dataManage.location.enterpriseDownKeyTemp.domain.EnterpriseDownKeyTemp" />
	<typeAlias alias="searchVo" type="com.tianque.plugin.dataManage.base.vo.SearchVo" />
	
	<resultMap id="enterpriseDownKeyTempResultForBase" class="EnterpriseDownKeyTemp" extends="enterpriseTemp.enterpriseTempResultForBase">
		<result property="industry.id" column="industry"/>
		<result property="businessLicense" column="businessLicense"/>
		<result property="area" column="area"/>
		<result property="registeredCapital" column="registeredCapital"/>
		<result property="employeeAmount" column="employeeAmount"/>
		<result property="partyMemberAmount" column="partyMemberAmount"/>
		<result property="enterpriseTelephone" column="enterpriseTelephone"/>
		<result property="fax" column="fax"/>
	</resultMap>
	
    <resultMap id="enterpriseDownKeyTempResultForList" class="EnterpriseDownKeyTemp" extends="enterpriseDownKeyTemp.enterpriseDownKeyTempResultForBase">
		<result property="claimDetail" resultMap="dataManage.dataManageResultMap"/>
	</resultMap>
	
	<sql id="claim_sql_where">
		from DM_enterpriseDownKey_Temp d where 1=1
		<include refid="dataManage.searchVoDataManage"/>
	</sql>
	<sql id="updateBusinessSql">
		industry = #industry.id#
		,businessLicense = #businessLicense#
		,area = #area#
		,registeredCapital = #registeredCapital#
		,employeeAmount = #employeeAmount#
		,partyMemberAmount = #partyMemberAmount#
		,enterpriseTelephone = #enterpriseTelephone#
		,fax = #fax#
		,telephone = #telephone#
		,mobileNumber = #mobileNumber#
		,isRiskEnterprise = #isRiskEnterprise#
		,hiddenCases = #hiddenCases#
		,hiddenRectification = #hiddenRectification#
		,attentionExtent=#attentionExtent.id#
	</sql>
	<insert id="addEnterpriseDownKeyTemp" parameterClass="EnterpriseDownKeyTemp">
		<selectKey resultClass="java.lang.Long" keyProperty="id">
			<![CDATA[SELECT s_DM_enterpriseDownKey_Temp.NEXTVAL as id from dual ]]>
		</selectKey>
		insert into DM_enterpriseDownKey_Temp
		(id,orgId,orgInternalCode,name,manager,address,industry,businessLicense,area,registeredCapital,
		 employeeAmount,partyMemberAmount,enterpriseTelephone,fax,telephone,mobileNumber,attentionExtent,
		 isRiskEnterprise,hiddenCases,hiddenRectification,remark,createDate,createUser,type,
		<include refid="dataManage.addDataManage"/>
		)
		values
		(#id#,#organization.id#,#orgInternalCode#,#name#,#manager#,#address#,#industry.id#,#businessLicense#,#area#,
		#registeredCapital#,#employeeAmount#,#partyMemberAmount#,#enterpriseTelephone#,#fax#,#telephone#,#mobileNumber#,#attentionExtent.id#,
		#isRiskEnterprise#,#hiddenCases#,#hiddenRectification#,#remark#,#createDate#,#createUser#,(select id from propertyDicts where displayName='规下企业'),
		<include refid="dataManage.addValueDataManage"/>
		)
    </insert>
	
	 <select id="findTempPageList" resultMap="enterpriseDownKeyTempResultForList"
		parameterClass="searchVo">
		<![CDATA[select d.*,'enterpriseDownKey' as keyType ]]>
		<include refid="claim_sql_where" />
		<dynamic prepend="order by">
			<isPropertyAvailable property="sortField">
				<isNotNull property="sortField">$sortField$</isNotNull>
				<isNotNull property="order">$order$</isNotNull>
			</isPropertyAvailable>
		</dynamic>
	</select>
	
	<update id="updateEnterpriseDownKeyTempInIdById"  parameterClass="java.util.Map">
		update DM_enterpriseDownKey_Temp set InId = #claimDetail.inid# where id=#id#
	</update>
	<update id="updateEnterpriseDownKeyTempClaimDetail"  parameterClass="java.util.Map">
		update DM_enterpriseDownKey_Temp set <include refid="dataManage.updateClaimDetail"/>
	</update>
	
	<update id="updateEnterpriseDownKeyTemp" parameterClass="EnterpriseDownKeyTemp">
		update DM_enterpriseDownKey_Temp set
		<include refid="locationTempBaseDomain.updateLocationTempBaseInfo"/>
		<include refid="dataManage.updateDataManage"/>
		,
		<include refid="enterpriseDownKeyTemp.updateBusinessSql"/>
		where id=#id#
	</update>
	
	<select id="findTemps" resultMap="enterpriseDownKeyTempResultForList"
		parameterClass="searchVo">
		<![CDATA[select d.*,'enterpriseDownKey' as keyType ]]>
		<include refid="claim_sql_where" />
		<dynamic prepend="order by">
			<isPropertyAvailable property="sortField">
				<isNotNull property="sortField">$sortField$</isNotNull>
				<isNotNull property="order">$order$</isNotNull>
			</isPropertyAvailable>
		</dynamic>
	</select>
	<select id="countFindTemps" resultClass="java.lang.Integer"
		parameterClass="searchVo">
			select count(id)  <include refid="claim_sql_where" />
	</select>
	<select id="getEnterpriseDownKeyTempById" parameterClass="long" resultMap="enterpriseDownKeyTempResultForList">
		select d.*,'enterpriseDownKey' as keyType from DM_enterpriseDownKey_Temp d where d.id=#value#
	</select>
	<!-- 修改场所信息 -->
	<update id="updateEnterpriseDownKeyTempInfo" parameterClass="EnterpriseDownKeyTemp">
		update DM_enterpriseDownKey_Temp set
		<include refid="locationTempBaseDomain.updateLocationTempBaseInfo"/>
		<include refid="dataManage.updateDataManage"/>
		,
		<include refid="enterpriseDownKeyTemp.updateBusinessSql"/>
		where id=#id#
	</update>
	<delete id="deleteEnterpriseDownKeyTempById" parameterClass="Long">
	   delete DM_enterpriseDownKey_Temp where id = #value#
	</delete>
</sqlMap>