<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.objectFactory" value="spring" />
	<package name="searchSpecialisationManageNew" extends="default" namespace="/specialisation/searchSpecialisationManageNew">
		<default-class-ref class="searchSpecialisationControllerNew"></default-class-ref>
		<action name="searchSpecialisation"	method="searchSpecialisation">
			<result name="success" type="json">
			     <param name="root">gridPage</param>
			     <param name="ignoreHierarchy">false</param>
			</result>
			<result name="error" type="json">
                <param name="root">errorMessage</param>
            </result>
		</action>
	</package>
</struts>
