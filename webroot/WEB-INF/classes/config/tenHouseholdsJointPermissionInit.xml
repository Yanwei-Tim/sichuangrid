<?xml version="1.0" encoding="UTF-8"?>
<!--
	description属性代表详细描述;
	permissionType代表为功能或是菜单权限{1:代表菜单;0：代表功能};
	ename属性代表对应权限的英文名;
	moduleName代表所属的模块;
	cname代表所对应的权限名称;
 -->
