.ui-layout-west{
	height:100%!important;
}
.ui-layout-center{
	padding-left:0;
}
.content{
	margin-left:0;
	padding-top:0;
}
.main{
	height:100%;
}
.mainBody{
	overflow:hidden;
	position:relative;
}
#mapMain{
	width:70%;
	text-align:left;
	float:left;
	
}
.rightPieMod{
	width:30%;
	height:70%;
	float:right;
}
.rightPieMod .tit{
	height:36px;
	border-top:1px solid #22c4ff;
	background-color: #1ba1e2; 
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#1eace6', endColorstr='#1ba1e2'); 
	background-image: -webkit-gradient(linear, left top, left bottom, from(#1eace6), to(#1ba1e2)); 
	background-image: -webkit-linear-gradient(top, #1eace6, #1ba1e2); 
	background-image: -moz-linear-gradient(top, #1eace6, #1ba1e2); 
	background-image: -ms-linear-gradient(top, #1eace6, #1ba1e2); 
	background-image: -o-linear-gradient(top, #1eace6, #1ba1e2); 
	background-image: linear-gradient(top, #1eace6, #1ba1e2); 
	box-shadow: 0 0 2px #e4e4e4;
}
.rightPieMod .tit h2{
	color:#fff;
	text-indent:15px;
	font:bold 14px/36px 'Micorsoft Yahei';
}
.rightPieMod .info{
	border:1px solid #e5e5e5;
	min-height:350px;
}
.rightPieMod .info li{
	min-height:39px; 
	height:auto !important; 
	height:39px; 
	overflow:visible;
	margin:0 10px;
	color:#333;
	text-indent:5px;
	font:14px/38px 'Microsoft Yahei';
	border-bottom:1px dashed #ccc;
}
.rightPieMod .info li em{
	font:normal 14px/38px 'Microsoft Yahei';
}
.rightPieMod .info li span{
	width:8px;
	height:12px;
	margin:0 18px 0 25px;
	display:inline-block;
	vertical-align: middle;
    font:14px/38px 'Microsoft Yahei';
	background:url(/resource/judgmentAnalysis/img/analysis/up_down.png) no-repeat;
	
}
.rightPieMod .info li span.up{
	background-position:0 0;
}
.rightPieMod .info li span.down{
	background-position:0 -40px;
}
.pieChartMod{
	border-bottom:0 none;
}
.bottomTabs{
	width:100%;
	height:85px;
	background:#9eaab8;
	filter: alpha(opacity=90);
	opacity: 0.9;
	position:absolute;
	bottom:0;
	clear:both;
}
.bottomTabs .space{
	width:50%;
	float:left;
	display:block;
}
.bottomTabs .tabs{
	float:right;
	width:50%;
	height:100%;
}
.bottomTabs .tabs .tabsItem{
	position:relative;
	float: left;
    width:33.33333333%;
    height: 100%;
    cursor:pointer;
    box-sizing:border-box;
	border-left:1px solid #cfd5dc;
}
.bottomTabs .tabs .tabsItem.on,.bottomTabs .tabs .tabsItem:hover{
	background: #3A5573;
}
.bottomTabs .tabs .tabsItem h2{
	position: relative;
	color:#fff;
	text-align: center;
	font:bold 16px/127px 'Microsoft Yahei';
}
.bottomTabs .tabs .tabsItem h2 i{
	position: absolute;
    left: 44%;
    top: 12%;
	display:block;
	width:36px;
	height:36px;
	background:url(/resource/judgmentAnalysis/img/analysis/icon_catory_people.png) no-repeat;
}
.bottomTabs .tabs .tabsItem h2 i.specialPopulation{
	background-position:0 0;
}
.bottomTabs .tabs .tabsItem h2 i.specialPopulation{
	background-position:0 -50px;
}
.bottomTabs .tabs .tabsItem h2 i.hiddenDanger{
	background-position:0 -93px;
}
.bottomTabs .tabs .tabsItem .item{
	position: absolute;
	left:0; 
	bottom:85px;
	display:none;
	width:100%;
	min-height:125px;
	background-color:#3d5672;
}
.bottomTabs .tabs .tabsItem .item .itemA,.tabs .tabsItem .item .subitem a{
	display: block;
	width:100%;
	height:40px;
	color:#fff;
	text-indent:15px;
	font:14px/40px 'Microsoft Yahei';
	background-color:#3d5672;
}
.bottomTabs .tabs .tabsItem .item .itemA:hover{
	background-color:#1ba1e2;
}
.tabs .tabsItem .item .subitem{
	position:absolute;
	left:0;
	bottom:0;
	width:100%;
	height:100%;
	display:none;
	z-index:2;
}
.tabs .tabsItem .item .subitem .subitemBg{
	background-color:#3d5672;
}
.tabs .tabsItem .item .subitem a{
	background-color:#1ba1e2;
}
.tabs .tabsItem .item .subitem a:hover{
	background-color:#1891cb;
}






