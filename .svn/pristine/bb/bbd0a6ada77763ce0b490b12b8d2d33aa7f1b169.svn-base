package com.tianque.service.vo;

import java.util.List;

public class MonthAndWeekWorkDayInfo {

	private int monthWorkDays;// 月的工作日天数
	private int[] weekWorkDays;// 每个周的工作天数
	private List<List<String>> weekWorkDayList;// 每个周的工作日的详细

	public int getMonthWorkDays() {
		return monthWorkDays;
	}

	public void setMonthWorkDays(int monthWorkDays) {
		this.monthWorkDays = monthWorkDays;
	}

	public int[] getWeekWorkDays() {
		return weekWorkDays;
	}

	public void setWeekWorkDays(int[] weekWorkDays) {
		this.weekWorkDays = weekWorkDays;
	}

	public List<List<String>> getWeekWorkDayList() {
		return weekWorkDayList;
	}

	public void setWeekWorkDayList(List<List<String>> weekWorkDayList) {
		this.weekWorkDayList = weekWorkDayList;
	}

}
