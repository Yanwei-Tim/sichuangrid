package com.tianque.solr.domain;

import java.io.Serializable;

import org.apache.solr.client.solrj.beans.Field;

public class KeyPopulationDocument implements Serializable {

	@Field
	private String idCardNo;
	@Field
	private Long[] orgIds;
	@Field
	private String[] orgInternalCodes;
	@Field
	private Long[] ids;
	@Field
	private DocumentType[] types;

	private String[] addresses;
	@Field
	private String name;
	@Field
	private String fullPinyin;
	@Field
	private String simplePinyin;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getIdCardNo() {
		return idCardNo;
	}

	public void setIdCardNo(String idCardNo) {
		this.idCardNo = idCardNo;
	}

	public String getFullPinyin() {
		return fullPinyin;
	}

	public void setFullPinyin(String fullPinyin) {
		this.fullPinyin = fullPinyin;
	}

	public String getSimplePinyin() {
		return simplePinyin;
	}

	public void setSimplePinyin(String simplePinyin) {
		this.simplePinyin = simplePinyin;
	}

	public Long[] getIds() {
		return ids;
	}

	public void setIds(Long[] ids) {
		this.ids = ids;
	}

	public DocumentType[] getTypes() {
		return types;
	}

	public void setTypes(DocumentType[] types) {
		this.types = types;
	}

	public Long[] getOrgIds() {
		return orgIds;
	}

	public void setOrgIds(Long[] orgIds) {
		this.orgIds = orgIds;
	}

	public String[] getOrgInternalCodes() {
		return orgInternalCodes;
	}

	public void setOrgInternalCodes(String[] orgInternalCodes) {
		this.orgInternalCodes = orgInternalCodes;
	}

	public String[] getAddresses() {
		return addresses;
	}

	public void setAddresses(String[] addresses) {
		this.addresses = addresses;
	}

}
