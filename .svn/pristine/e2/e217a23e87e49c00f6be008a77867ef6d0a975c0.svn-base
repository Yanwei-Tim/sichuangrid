package com.tianque.baseInfo.idleYouth.domain;

import java.util.List;

import com.tianque.baseInfo.base.domain.AttentionPopulation;
import com.tianque.core.util.BaseInfoTables;
import com.tianque.domain.PropertyDict;

@SuppressWarnings("serial")
public class IdleYouth extends AttentionPopulation {
	private transient List<PropertyDict> staffType;
	private String workCondition;
	private boolean isStayInSchool;
	private String schoolName;
	/** 监护人 */
	private String guardianName;
	/** 联系方式 */
	private String guardianTelephone;
	/** 帮扶情况 */
	private PropertyDict helpingSituation;

	public IdleYouth() {
		setAttentionPopulationType(BaseInfoTables.IDLEYOUTH_KEY);
	}

	public List<PropertyDict> getStaffType() {
		return staffType;
	}

	public void setStaffType(List<PropertyDict> staffType) {
		this.staffType = staffType;
	}

	public String getWorkCondition() {
		return workCondition;
	}

	public void setWorkCondition(String workCondition) {
		this.workCondition = workCondition;
	}

	public boolean isStayInSchool() {
		return isStayInSchool;
	}

	public void setStayInSchool(boolean isStayInSchool) {
		this.isStayInSchool = isStayInSchool;
	}

	public String getSchoolName() {
		return schoolName;
	}

	public void setSchoolName(String schoolName) {
		this.schoolName = schoolName;
	}

	public String getGuardianName() {
		return guardianName;
	}

	public void setGuardianName(String guardianName) {
		this.guardianName = guardianName;
	}

	public String getGuardianTelephone() {
		return guardianTelephone;
	}

	public void setGuardianTelephone(String guardianTelephone) {
		this.guardianTelephone = guardianTelephone;
	}

	public PropertyDict getHelpingSituation() {
		return helpingSituation;
	}

	public void setHelpingSituation(PropertyDict helpingSituation) {
		this.helpingSituation = helpingSituation;
	}

}
