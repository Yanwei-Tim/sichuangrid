package com.tianque.incident.domain;

import java.util.List;

import com.tianque.core.base.BaseDomain;
import com.tianque.domain.PropertyDict;
import com.tianque.incident.domain.component.IncidentDegreeRule;

public class IncidentType extends BaseDomain {

	/** 树的核心 */
	private Long subCount;
	private int seq;

	private String name;
	private PropertyDict subjection;

	private String description;

	private Boolean degreed;

	private List<IncidentDegreeRule> degreeRule;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public PropertyDict getSubjection() {
		return subjection;
	}

	public void setSubjection(PropertyDict subjection) {
		this.subjection = subjection;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public Boolean getDegreed() {
		return degreed;
	}

	public void setDegreed(Boolean degreed) {
		this.degreed = degreed;
	}

	public List<IncidentDegreeRule> getDegreeRule() {
		return degreeRule;
	}

	public void setDegreeRule(List<IncidentDegreeRule> degreeRule) {
		this.degreeRule = degreeRule;
	}

	public Long getSubCount() {
		return subCount;
	}

	public void setSubCount(Long subCount) {
		this.subCount = subCount;
	}

	public int getSeq() {
		return seq;
	}

	public void setSeq(int seq) {
		this.seq = seq;
	}

}
