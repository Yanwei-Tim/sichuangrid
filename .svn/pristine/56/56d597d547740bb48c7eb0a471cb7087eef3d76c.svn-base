package com.tianque.newVillage.domain;

import com.tianque.core.base.BaseDomain;
import com.tianque.domain.Organization;

/**
 * @ClassName: EnvironmentalReform
 * @Description: 环境改造
 */
public class EnvironmentalReform extends BaseDomain {
	private Organization organization;// 组织机构.
	private NewVillageBasic newVillageBasic;// 基本信息

	private Integer garbageTank;// 垃圾池个数
	private Integer toilets;// 公厕
	private Integer treatmentFacilities;// 污水处理设施
	
	private Integer sanitarySewage;//生活污水处理户数
	private Integer garbageDisposal;//是否生活垃圾处理全覆盖
	private Integer  treatmentPollution;//是否面源污染有效治理

	public Integer getSanitarySewage() {
		return sanitarySewage==null?0:sanitarySewage;
	}

	public void setSanitarySewage(Integer sanitarySewage) {
		this.sanitarySewage = sanitarySewage;
	}

	public Integer getGarbageDisposal() {
		return garbageDisposal==null?0:garbageDisposal;
	}

	public void setGarbageDisposal(Integer garbageDisposal) {
		this.garbageDisposal = garbageDisposal;
	}

	public Integer getTreatmentPollution() {
		return treatmentPollution==null?0:treatmentPollution;
	}

	public void setTreatmentPollution(Integer treatmentPollution) {
		this.treatmentPollution = treatmentPollution;
	}

	public Organization getOrganization() {
		return organization;
	}

	public void setOrganization(Organization organization) {
		this.organization = organization;
	}

	public NewVillageBasic getNewVillageBasic() {
		return newVillageBasic;
	}

	public void setNewVillageBasic(NewVillageBasic newVillageBasic) {
		this.newVillageBasic = newVillageBasic;
	}

	public Integer getGarbageTank() {
		return garbageTank == null ? 0 : garbageTank;
	}

	public void setGarbageTank(Integer garbageTank) {
		this.garbageTank = garbageTank;
	}

	public Integer getToilets() {
		return toilets == null ? 0 : toilets;
	}

	public void setToilets(Integer toilets) {
		this.toilets = toilets;
	}

	public Integer getTreatmentFacilities() {
		return treatmentFacilities == null ? 0 : treatmentFacilities;
	}

	public void setTreatmentFacilities(Integer treatmentFacilities) {
		this.treatmentFacilities = treatmentFacilities;
	}

}
