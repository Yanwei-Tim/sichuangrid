/* jGrowl */
.jGrowl{z-index:9999;text-align:center;position:absolute;left:50%;top:-70px;margin-left:-125px;width:250px;height:auto;overflow:hidden;}
.jGrowl .info,
.jGrowl .error,
.jGrowl .success,
.jGrowl .warn{color:#fff;font-size: 12px;font-weight: bold;line-height: 20px;border-radius: 0 0 3px 3px;padding:5px 10px;}
.jGrowl .info,.jGrowl .error,.jGrowl .success,.jGrowl .warn{background:url(/resource/system/images/jGrow-bg.png) repeat;}
.jGrowl span{background:url(/resource/system/images/jGrowl.png) no-repeat;width:22px;height:22px;display:inline-block;*display:inline;*zoom:1;vertical-align:middle;}
.jGrowl .info span{background:none;}
.jGrowl .warn span{background-position:0 -26px}
