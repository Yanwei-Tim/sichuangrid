/*  orgSelectBox Style  */

.orgBoxResult a{margin-left:-3px; margin-right:6px;}
.orgBoxResult a.cur{font-weight:bold;}
.orgBoxSearchBox{}
.orgLoadBox{width:100%;height:100%;position:absolute;top:0;left:0;z-index:1;background:#F4F5F5;text-align:center;}
.orgLoadBox img{margin-top:45px;}
.organizationBox h3{width:100px;}
.organizationBox h3,.organizationlist{display:inline-block;}
.organizationBox a{margin:0 3px;}
.organizationBox a.cur{}
.orgSelectBox{width:520px; border:0 none;min-height:100px;_height:100px;}
.orgSelectBox .orgBoxSearchBox{line-height:28px;}
.orgBoxSearchBox .organizationBox{background:#fff;border:1px solid #E6E6E6;margin-top:-1px;padding:10px 5px;}
.orgBoxSearchBox .organizationBox h3{display:inline-block; *zoom:1; *display:inline; width:85px; vertical-align:top; text-indent:6px;}
.orgBoxSearchBox .organizationlist{display:inline-block; *zoom:1; *display:inline; width:415px;height:auto !important;height:150px;max-height:150px;overflow: auto;}
.orgBoxSearchBox .organizationlist a{text-decoration:underline;margin:0 10px 0 4px; color:#5f5f5f; display:inline-block; *zoom:1; *display:inline;}
.orgBoxSearchBox .organizationlist a.cur{color:#4b86d2;}
.orgSelectBox .orgBoxResult{_width:100%;position:relative;overflow:hidden;}
.orgSelectBox .orgBoxResult a {
position:relative;
display:block;float:left;padding-bottom:6px;}
.orgSelectBox .orgBoxResult a strong{margin:5px 5px 0 5px; padding:3px 6px;text-align:center;display:inline-block;white-space:nowrap;*zoom:1; *display:inline; min-width:45px; height:16px; line-height:16px;}
.orgSelectBox .orgBoxResult a.cur,.orgSelectBox .orgBoxResult a:hover{background:url(/resource/system/images/orgbox_arrow.gif) no-repeat center bottom;}
.orgSelectBox .orgBoxResult a.cur strong{color:#fff;filter:none;}
.orgSelectBox .orgBoxResult a:hover strong{cursor:pointer;}

/*orgSelectBox*/
.orgBoxSearchBox .organizationlist a:hover{color:#4b86d2;}
.orgSelectBox .orgBoxResult a strong{color:#4b86d2;border:1px solid #ccc;background:-moz-linear-gradient(top, #fafafa, #e7eef1); background:-webkit-gradient(linear, left top, left bottom, from(#fafafa), to(#e7eef1)); filter:progid:DXImageTransform.Microsoft.Gradient(GradientType=0,StartColorStr='#fafafa',EndColorStr='#e7eef1');}
.orgSelectBox .orgBoxResult a.cur strong{background:#4687DE;}
.orgSelectBox .orgBoxResult .arrows{
    position:absolute;
    left: -5px;
    top: 13px;
    width:6px;
    height:9px;
    background:url(/resource/system/images/orgBoxResultIcon.png) 0 0 no-repeat;
}

