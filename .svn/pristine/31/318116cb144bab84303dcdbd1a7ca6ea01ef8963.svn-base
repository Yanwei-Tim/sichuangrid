.ui-dialog { padding: 0; border: 0 none; background: #f4f5f5; position:absolute !important;}
.ui-dialog .ui-dialog-title { font-size: 16px; color: #fff; margin: 0; line-height: 38px;}
.ui-widget-overlay {
    position: absolute;top: 0;left: 0; background: #000 \9; opacity: .6; filter: Alpha(Opacity=60);
    background-image: -webkit-radial-gradient(hsla(120,70%,60%,.9), hsla(360,60%,60%,.9));
    background-image: radial-gradient(hsla(0, 0%, 51%, 0.9), hsla(0, 1%, 6%, 0.9));
}
.ui-dialog .ui-widget-content {
    overflow: auto;
    background: #f4f5f5;
    padding: 0 10px;
}
.ui-dialog .ui-dialog-buttonpane {
    padding: 5px 0;
    text-align: center;
    border-top: 1px solid #eee;
}
.ui-dialog .ui-dialog-titlebar,
.ui-dialog .ui-dialog-buttonpane button {
    font: 14px/38px 'microsoft yahei' ; border: 0 none;
}
.ui-dialog .ui-dialog-titlebar {
    cursor: pointer;
    padding: 0 1em;
    background-color: #ff3f25;
    background: url(../img/newStyle/icon_dialog_titBg.png) 0 0 repeat-x;
}
.ui-dialog .ui-dialog-buttonpane button {
    position: relative;
    margin: 0 20px;
    height: 32px;
    line-height: 32px;
    font-weight: bold;
    border-radius: 3px;
    text-align: center;
    cursor: pointer;
    Letter-spacing:5px;
    padding: 0 10px 0 15px;
    background: url(../img/newStyle/icon_dialog_bottomBtn.png) 0 0 repeat-x;
    border: 1px solid #2191e0;
    box-shadow: inset 0px 0px 1px #FFFFFF;
}
.ui-dialog .ui-dialog-buttonpane button span { font: 14px/20px 'microsoft yahei' ; color: #fff; }
.ui-dialog .ui-dialog-buttonpane button.ui-state-hover { top: 1px;}
.ui-dialog .ui-dialog-buttonpane button.butFocus { background-color: #ff3f25; }
.ui-dialog .ui-dialog-buttonpane button.butFocus.ui-state-hover {}
.ui-dialog .ui-dialog-buttonpane button.butFocus span {}
.ui-dialog .ui-dialog-titlebar-close { position: absolute; right: 0; top: 0; margin: 0; width: 36px; height: 34px; }
.ui-dialog .ui-dialog-titlebar-close:focus,
.ui-dialog .ui-dialog-titlebar-close:hover {
    top:2px;
    background:none;
    border: 0 none;
    right: 0;
    padding-right: 1px;}
.ui-dialog .ui-dialog-titlebar-close span{
    position: relative; width: 36px; height: 34px; overflow: hidden; display: block; text-indent: -999em;margin:0;
    background: url('../img/newStyle/icon_dialog_close.png') center no-repeat;
}

