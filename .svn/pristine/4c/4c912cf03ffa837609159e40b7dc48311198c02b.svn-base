<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4"
	xmlns="http://java.sun.com/xml/ns/j2ee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee
	http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">

	<!--注册-->
	<servlet>
		<servlet-name>initEnvironmentAndGetTicketMesg</servlet-name>
		<servlet-class>com.tianque.install.servlet.InitEnvironmentAndGetTicketMesg</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>initEnvironmentAndGetTicketMesg</servlet-name>
		<url-pattern>/initEnvironmentAndGetTicketMesg</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>fileDirectorServlet</servlet-name>
		<servlet-class>com.tianque.install.servlet.FileDirectorServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>fileDirectorServlet</servlet-name>
		<url-pattern>/fileDirectorServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>initJdbcServlet</servlet-name>
		<servlet-class>com.tianque.install.servlet.InitJdbcServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>initJdbcServlet</servlet-name>
		<url-pattern>/initJdbcServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>initMemcatchedServlet</servlet-name>
		<servlet-class>com.tianque.install.servlet.InitMemcatchedServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>initMemcatchedServlet</servlet-name>
		<url-pattern>/initMemcatchedServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>functionServlet</servlet-name>
		<servlet-class>com.tianque.install.servlet.FunctionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>functionServlet</servlet-name>
		<url-pattern>/functionServlet</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>/index.jsp</welcome-file>
	</welcome-file-list>

</web-app>
