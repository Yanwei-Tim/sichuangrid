package com.tianque.newVillage.vo;

import com.tianque.core.base.BaseDomain;

public class OrganizationConstructionVo extends BaseDomain {

	/** 基层组织建设字段 */
	private int isPositionBuilding;// 是否阵地建设(0无 1有)
	private int threeSatisfaction;// 三务公开群众满意度
	private int surveySatisfaction;// 调查满意度

	public int getIsPositionBuilding() {
		return isPositionBuilding;
	}

	public void setIsPositionBuilding(int isPositionBuilding) {
		this.isPositionBuilding = isPositionBuilding;
	}

	public int getThreeSatisfaction() {
		return threeSatisfaction;
	}

	public void setThreeSatisfaction(int threeSatisfaction) {
		this.threeSatisfaction = threeSatisfaction;
	}

	public int getSurveySatisfaction() {
		return surveySatisfaction;
	}

	public void setSurveySatisfaction(int surveySatisfaction) {
		this.surveySatisfaction = surveySatisfaction;
	}

}
