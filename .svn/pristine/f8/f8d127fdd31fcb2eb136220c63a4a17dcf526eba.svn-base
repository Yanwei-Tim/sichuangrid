package com.tianque.baseInfo.otherAttentionPersonnel.domain;

import com.tianque.baseInfo.base.domain.AttentionPopulation;
import com.tianque.core.util.BaseInfoTables;

public class OtherAttentionPersonnel extends AttentionPopulation {

	public OtherAttentionPersonnel() {
		setAttentionPopulationType(BaseInfoTables.OTHERATTENTIONPERSONNEL_KEY);
	}

	/**
	 * 关注原因
	 */
	private String attentionReason;

	/**
	 * 工作情况
	 */
	private String workCondition;

	private String meRemark;

	public String getAttentionReason() {
		return attentionReason;
	}

	public void setAttentionReason(String attentionReason) {
		this.attentionReason = attentionReason;
	}

	public String getWorkCondition() {
		return workCondition;
	}

	public void setWorkCondition(String workCondition) {
		this.workCondition = workCondition;
	}

	public String getMeRemark() {
		return meRemark;
	}

	public void setMeRemark(String meRemark) {
		this.meRemark = meRemark;
	}

}
