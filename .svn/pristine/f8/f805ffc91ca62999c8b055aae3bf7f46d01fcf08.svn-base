package com.tianque.approval.domain;

import com.tianque.core.base.BaseDomain;
import com.tianque.domain.PropertyDict;

public class Item extends BaseDomain {

	/**
	 * 事项编号
	 */
	private String itemNumber;
	/**
	 * 事项名称
	 */
	private String itemName;
	/**
	 * 全拼
	 */
	private String fullPinyin;
	/**
	 * 简拼
	 */
	private String simplePinyin;
	/**
	 * 承诺时限
	 */
	private Long promiseTime;
	/**
	 * 法定时限
	 */
	private Long legalTime;
	/**
	 * 是否收费项目
	 */
	private boolean isFees;
	/**
	 * 收费标准
	 */
	private Long standardFees;
	/**
	 * 事项分类
	 */
	private PropertyDict matterKind;
	/**
	 * 法定依据
	 */
	private String legalBasis;
	/**
	 * 办事指南
	 */
	private String lawGuide;
	/**
	 * 备注
	 */
	private String remark;

	public String getItemNumber() {
		return itemNumber;
	}

	public void setItemNumber(String itemNumber) {
		this.itemNumber = itemNumber;
	}

	public String getItemName() {
		return itemName;
	}

	public void setItemName(String itemName) {
		this.itemName = itemName;
	}

	public Long getPromiseTime() {
		return promiseTime;
	}

	public void setPromiseTime(Long promiseTime) {
		this.promiseTime = promiseTime;
	}

	public Long getLegalTime() {
		return legalTime;
	}

	public void setLegalTime(Long legalTime) {
		this.legalTime = legalTime;
	}

	public boolean isFees() {
		return isFees;
	}

	public void setFees(boolean isFees) {
		this.isFees = isFees;
	}

	public Long getStandardFees() {
		return standardFees;
	}

	public void setStandardFees(Long standardFees) {
		this.standardFees = standardFees;
	}

	public PropertyDict getMatterKind() {
		return matterKind;
	}

	public void setMatterKind(PropertyDict matterKind) {
		this.matterKind = matterKind;
	}

	public String getLegalBasis() {
		return legalBasis;
	}

	public void setLegalBasis(String legalBasis) {
		this.legalBasis = legalBasis;
	}

	public String getLawGuide() {
		return lawGuide;
	}

	public void setLawGuide(String lawGuide) {
		this.lawGuide = lawGuide;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public String getFullPinyin() {
		return fullPinyin;
	}

	public void setFullPinyin(String fullPinyin) {
		this.fullPinyin = fullPinyin;
	}

	public String getSimplePinyin() {
		return simplePinyin;
	}

	public void setSimplePinyin(String simplePinyin) {
		this.simplePinyin = simplePinyin;
	}
}
