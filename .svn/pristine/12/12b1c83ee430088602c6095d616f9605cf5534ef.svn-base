package com.tianque.baseInfo.orgLocationTrack.domain;

import java.util.Date;

import org.apache.struts2.json.annotations.JSON;

import com.tianque.core.base.BaseDomain;
import com.tianque.domain.Organization;

public class OrgLocationTracks extends BaseDomain {
	private Long orgLocationId;
	private String orgLocationName;
	private String orgLocationType;
	private Integer orgLocationinitType;
	private Organization orgLocationOrganization;
	private String orgLocationOrgInternalCode;
	private Organization oldOrgLocationOrganization;
	private Integer operationType;
	private String operationContent;
	private Long operationUserId;
	private Organization operationOrganization;
	private Date operationDate;

	public Long getOrgLocationId() {
		return orgLocationId;
	}

	public void setOrgLocationId(Long orgLocationId) {
		this.orgLocationId = orgLocationId;
	}

	public String getOrgLocationName() {
		return orgLocationName;
	}

	public void setOrgLocationName(String orgLocationName) {
		this.orgLocationName = orgLocationName;
	}

	public String getOrgLocationType() {
		return orgLocationType;
	}

	public void setOrgLocationType(String orgLocationType) {
		this.orgLocationType = orgLocationType;
	}

	public Integer getOrgLocationinitType() {
		return orgLocationinitType;
	}

	public void setOrgLocationinitType(Integer orgLocationinitType) {
		this.orgLocationinitType = orgLocationinitType;
	}

	public Organization getOrgLocationOrganization() {
		return orgLocationOrganization;
	}

	public void setOrgLocationOrganization(Organization orgLocationOrganization) {
		this.orgLocationOrganization = orgLocationOrganization;
	}

	public String getOrgLocationOrgInternalCode() {
		return orgLocationOrgInternalCode;
	}

	public void setOrgLocationOrgInternalCode(String orgLocationOrgInternalCode) {
		this.orgLocationOrgInternalCode = orgLocationOrgInternalCode;
	}

	public Organization getOldOrgLocationOrganization() {
		return oldOrgLocationOrganization;
	}

	public void setOldOrgLocationOrganization(
			Organization oldOrgLocationOrganization) {
		this.oldOrgLocationOrganization = oldOrgLocationOrganization;
	}

	public Integer getOperationType() {
		return operationType;
	}

	public void setOperationType(Integer operationType) {
		this.operationType = operationType;
	}

	public String getOperationContent() {
		return operationContent;
	}

	public void setOperationContent(String operationContent) {
		this.operationContent = operationContent;
	}

	public Long getOperationUserId() {
		return operationUserId;
	}

	public void setOperationUserId(Long operationUserId) {
		this.operationUserId = operationUserId;
	}

	public Organization getOperationOrganization() {
		return operationOrganization;
	}

	public void setOperationOrganization(Organization operationOrganization) {
		this.operationOrganization = operationOrganization;
	}

	@JSON(format = "yyyy-MM-dd HH:mm:ss")
	public Date getOperationDate() {
		return operationDate;
	}

	public void setOperationDate(Date operationDate) {
		this.operationDate = operationDate;
	}
}