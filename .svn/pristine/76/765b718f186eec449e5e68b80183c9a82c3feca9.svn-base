package com.tianque.plugin.analyzing.domain;

import java.util.Date;

import com.tianque.core.base.BaseDomain;

public class BaseInfoStatisticHistory extends BaseDomain {
	private Date startDate;
	private Date endDate;
	private Integer year;
	private Integer month;
	private String typeName;// 分类的名称
	private long sum;// 本组的数据总和，按照orgId和typeName分组之后的记录
	private String baseinfoType;// 基本表的type
	private String orgInternalCode;
	private Long orgId;// 本条记录的orgId
	private String orgName;
	private long isHelp;
	private long noHelp;
	private long resited;
	private long recidivism;// 本年度重犯数量
	private long countValue;// 统计的字段的值，统计的字段是StatisticListSetting中的keyName

	public long getSum() {
		return sum;
	}

	public void setSum(long sum) {
		this.sum = sum;
	}

	public long getIsHelp() {
		return isHelp;
	}

	public void setIsHelp(long isHelp) {
		this.isHelp = isHelp;
	}

	public long getNoHelp() {
		return noHelp;
	}

	public void setNoHelp(long noHelp) {
		this.noHelp = noHelp;
	}

	public long getResited() {
		return resited;
	}

	public void setResited(long resited) {
		this.resited = resited;
	}

	public long getRecidivism() {
		return recidivism;
	}

	public void setRecidivism(long recidivism) {
		this.recidivism = recidivism;
	}

	public long getCountValue() {
		return countValue;
	}

	public void setCountValue(long countValue) {
		this.countValue = countValue;
	}

	public Integer getYear() {
		return year;
	}

	public void setYear(Integer year) {
		this.year = year;
	}

	public Integer getMonth() {
		return month;
	}

	public void setMonth(Integer month) {
		this.month = month;
	}

	public String getTypeName() {
		return typeName;
	}

	public void setTypeName(String typeName) {
		this.typeName = typeName;
	}

	public String getBaseinfoType() {
		return baseinfoType;
	}

	public void setBaseinfoType(String baseinfoType) {
		this.baseinfoType = baseinfoType;
	}

	public String getOrgInternalCode() {
		return orgInternalCode;
	}

	public void setOrgInternalCode(String orgInternalCode) {
		this.orgInternalCode = orgInternalCode;
	}

	public Long getOrgId() {
		return orgId;
	}

	public void setOrgId(Long orgId) {
		this.orgId = orgId;
	}

	public String getOrgName() {
		return orgName;
	}

	public void setOrgName(String orgName) {
		this.orgName = orgName;
	}

	public Date getStartDate() {
		return startDate;
	}

	public void setStartDate(Date startDate) {
		this.startDate = startDate;
	}

	public Date getEndDate() {
		return endDate;
	}

	public void setEndDate(Date endDate) {
		this.endDate = endDate;
	}

}
