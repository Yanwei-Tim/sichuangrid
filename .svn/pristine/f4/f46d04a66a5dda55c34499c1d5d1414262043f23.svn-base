<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap 
PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
"http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="CommonHandler">

    <select id="getCount" parameterClass="java.lang.String" resultClass="java.lang.Integer">
		$value$
	</select>
	
	<select id="queryStringForList" parameterClass="java.lang.String" resultClass="java.lang.String">
		$value$
	</select>
	
	<update id="updateData" parameterClass="java.lang.String">
		$value$
	</update>
	
	<delete id="deleteData" parameterClass="java.lang.String">
		$value$
	</delete>
</sqlMap>