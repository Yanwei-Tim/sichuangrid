package com.tianque.init.impl.project;

import java.util.Map;

import com.tianque.core.globalSetting.service.GlobalSettingService;
import com.tianque.core.globalSetting.util.GlobalSetting;
import com.tianque.init.impl.GlobalSettingInitialization;

public class YFDGlobalSettingInit extends GlobalSettingInitialization {

	public YFDGlobalSettingInit(GlobalSettingService globalSettingService) {
		super(globalSettingService);
	}

	public Map<String, String> getInitDataMap() {
		Map<String, String> map = super.getInitDataMap();
		map.put(GlobalSetting.SYS_TITLE, "智慧羊坊店社会服务管理系统");
		map.put(GlobalSetting.SYS_HEADER_PAGE, "智慧羊坊店社会服务管理系统");
		map.put(GlobalSetting.SYS_BOTTOM_PAGE, "技术支持：北京中海纪元数字技术发展股份有限公司");
		map.put(GlobalSetting.FRONTPAGE_BOTTOM_PAGE, "技术支持：北京中海纪元数字技术发展股份有限公司");
		return map;
	}
}
