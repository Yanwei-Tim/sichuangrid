package com.tianque.plugin.account.domain;

import java.util.Date;

import com.tianque.core.base.BaseDomain;
import com.tianque.domain.Organization;
import com.tianque.domain.PropertyDict;

/**
 * 受理单
 * 
 * @author N-287
 */
public class AcceptForm extends BaseDomain {
	private Long stepId; // 步骤编号
	private Long ledgerId;// 台账编号
	private Long ledgerType; // 台账类型
	private Organization acceptOrg;// 受理部门
	private Date formComeDate;// 来件时间
	private Date formAcceptDate; // 接件时间
	private PropertyDict formType;// 来件类型
	private String currentOrgOpinion;// 本级台账办拟办意见
	private String inChargeOfLeaderOpinion;// 分管领导意见
	private String majorLeaderOpinion;// 主要领导意见
	private String handleResult;// 办理结果
	private String serialNumber; // 呈报单编码
	private String dealUserName; // 承办人

	public Long getStepId() {
		return stepId;
	}

	public void setStepId(Long stepId) {
		this.stepId = stepId;
	}

	public Long getLedgerId() {
		return ledgerId;
	}

	public void setLedgerId(Long ledgerId) {
		this.ledgerId = ledgerId;
	}

	public Organization getAcceptOrg() {
		return acceptOrg;
	}

	public void setAcceptOrg(Organization acceptOrg) {
		this.acceptOrg = acceptOrg;
	}

	public Date getFormComeDate() {
		return formComeDate;
	}

	public void setFormComeDate(Date formComeDate) {
		this.formComeDate = formComeDate;
	}

	public Date getFormAcceptDate() {
		return formAcceptDate;
	}

	public void setFormAcceptDate(Date formAcceptDate) {
		this.formAcceptDate = formAcceptDate;
	}

	public Long getLedgerType() {
		return ledgerType;
	}

	public void setLedgerType(Long ledgerType) {
		this.ledgerType = ledgerType;
	}

	public PropertyDict getFormType() {
		return formType;
	}

	public void setFormType(PropertyDict formType) {
		this.formType = formType;
	}

	public String getCurrentOrgOpinion() {
		return currentOrgOpinion;
	}

	public void setCurrentOrgOpinion(String currentOrgOpinion) {
		this.currentOrgOpinion = currentOrgOpinion;
	}

	public String getInChargeOfLeaderOpinion() {
		return inChargeOfLeaderOpinion;
	}

	public void setInChargeOfLeaderOpinion(String inChargeOfLeaderOpinion) {
		this.inChargeOfLeaderOpinion = inChargeOfLeaderOpinion;
	}

	public String getMajorLeaderOpinion() {
		return majorLeaderOpinion;
	}

	public void setMajorLeaderOpinion(String majorLeaderOpinion) {
		this.majorLeaderOpinion = majorLeaderOpinion;
	}

	public String getHandleResult() {
		return handleResult;
	}

	public void setHandleResult(String handleResult) {
		this.handleResult = handleResult;
	}

	public String getSerialNumber() {
		return serialNumber;
	}

	public void setSerialNumber(String serialNumber) {
		this.serialNumber = serialNumber;
	}

	public String getDealUserName() {
		return dealUserName;
	}

	public void setDealUserName(String dealUserName) {
		this.dealUserName = dealUserName;
	}

}