package com.tianque.sms.domain.vo;

import java.io.Serializable;

/**
 * 垃圾短信箱:SrarchVO(SMSTRASH)
 * 
 * @author
 * @date 2013-09-22 16:42:50
 */
public class SearchSmstrashVo implements Serializable {
	private static final long serialVersionUID = 1L;

	private String id;
	/** 最大 发送或接收时间 */
	private java.util.Date maxTime;
	/** 最小 发送或接收时间 */
	private java.util.Date minTime;
	/** 号码 */
	private String mobile;
	/** 短信来源类型：1发件箱，2收件箱 */
	private Long fromType;
	/** 短信内容 */
	private String content;

	/** get 最大 发送或接收时间 */
	public java.util.Date getMaxTime() {
		return maxTime;
	}

	/** set 最大 发送或接收时间 */
	public void setMaxTime(java.util.Date maxTime) {
		this.maxTime = maxTime;
	}

	/** get 最小 发送或接收时间 */
	public java.util.Date getMinTime() {
		return minTime;
	}

	/** set 最小 发送或接收时间 */
	public void setMinTime(java.util.Date minTime) {
		this.minTime = minTime;
	}

	/** get 号码 */
	public String getMobile() {
		return mobile;
	}

	/** set 号码 */
	public void setMobile(String mobile) {
		this.mobile = mobile;
	}

	/** get 短信来源类型：1发件箱，2收件箱 */
	public Long getFromType() {
		return fromType;
	}

	/** set 短信来源类型：1发件箱，2收件箱 */
	public void setFromType(Long fromType) {
		this.fromType = fromType;
	}

	/** get 短信内容 */
	public String getContent() {
		return content;
	}

	/** set 短信内容 */
	public void setContent(String content) {
		this.content = content;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

}
