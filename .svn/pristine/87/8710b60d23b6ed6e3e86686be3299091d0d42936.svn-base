package com.tianque.domain.vo;

import java.io.Serializable;
import java.util.Date;

import com.tianque.domain.Organization;

public class PublicNoticeVo implements Serializable {
	/* 区域 */
	private Organization organization;
	/* 区域编号 */
	private String organizationCode;
	/* 检索的起始时间 */
	private Date startDate;
	/* 检索的结束时间 */
	private Date endDate;
	/* 标题 */
	private String editorTitle;
	/* 本级或下辖 */
	private Integer publicNoticeLevel;
	/* 查询的编辑时间 */
	private Date editstartDate;
	private Date editEndDate;
	/* 查询的有效期时间 */
	private Date validoverdueStartDate;
	private Date validoverdueEndDate;
	/* 编辑者 */
	private String noticeEditor;
	/* 是否在有效期内 */
	private Integer isInValidity;
	/* 是否有显示有效期 */
	private Integer isHaveValidity;

	public Organization getOrganization() {
		return organization;
	}

	public void setOrganization(Organization organization) {
		this.organization = organization;
	}

	public String getOrganizationCode() {
		return organizationCode;
	}

	public void setOrganizationCode(String organizationCode) {
		this.organizationCode = organizationCode;
	}

	public Date getStartDate() {
		return startDate;
	}

	public void setStartDate(Date startDate) {
		this.startDate = startDate;
	}

	public Date getEndDate() {
		return endDate;
	}

	public void setEndDate(Date endDate) {
		this.endDate = endDate;
	}

	public String getEditorTitle() {
		return editorTitle;
	}

	public void setEditorTitle(String editorTitle) {
		this.editorTitle = editorTitle;
	}

	public Integer getPublicNoticeLevel() {
		return publicNoticeLevel;
	}

	public void setPublicNoticeLevel(Integer publicNoticeLevel) {
		this.publicNoticeLevel = publicNoticeLevel;
	}

	public Date getEditstartDate() {
		return editstartDate;
	}

	public void setEditstartDate(Date editstartDate) {
		this.editstartDate = editstartDate;
	}

	public Date getEditEndDate() {
		return editEndDate;
	}

	public void setEditEndDate(Date editEndDate) {
		this.editEndDate = editEndDate;
	}

	public Date getValidoverdueStartDate() {
		return validoverdueStartDate;
	}

	public void setValidoverdueStartDate(Date validoverdueStartDate) {
		this.validoverdueStartDate = validoverdueStartDate;
	}

	public Date getValidoverdueEndDate() {
		return validoverdueEndDate;
	}

	public void setValidoverdueEndDate(Date validoverdueEndDate) {
		this.validoverdueEndDate = validoverdueEndDate;
	}

	public String getNoticeEditor() {
		return noticeEditor;
	}

	public void setNoticeEditor(String noticeEditor) {
		this.noticeEditor = noticeEditor;
	}

	public Integer getIsInValidity() {
		return isInValidity;
	}

	public void setIsInValidity(Integer isInValidity) {
		this.isInValidity = isInValidity;
	}

	public Integer getIsHaveValidity() {
		return isHaveValidity;
	}

	public void setIsHaveValidity(Integer isHaveValidity) {
		this.isHaveValidity = isHaveValidity;
	}

}
