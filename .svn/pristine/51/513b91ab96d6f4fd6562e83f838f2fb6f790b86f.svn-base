package com.tianque.plugin.account.domain;

import java.util.Date;
import java.util.List;

import org.apache.struts2.json.annotations.JSON;

import com.tianque.domain.PropertyDict;

public class LedgerSteadyWork extends BaseWorking {

	/*** 涉稳群体人数 */
	private Integer involvingSteadyNum;
	/*** 涉稳人群类别 */
	private PropertyDict involvingSteadyType;
	/*** 涉稳事项 */
	private String involvingSteadyInfo;
	/*** 稳控责任单位 */
	private String steadyUnit;
	/*** 稳控责任人 */
	private String steadyUser;
	/*** 化解责任部门 */
	private String resolveUnit;
	/*** 化解责任人 */
	private String resolveUser;
	/*** 诉求类别 */
	private PropertyDict aspirationType;
	/*** 涉稳人员或群体稳定现状 */
	private String steadyInfo;
	/*** 关注程度 */
	private Integer attentionLevel;
	/*** 涉稳类型 （大） */
	private PropertyDict steadyWorkType;
	/*** 涉稳问题类型 （小） */
	private PropertyDict steadyWorkProblemType;
	/*** 涉稳预警级别 */
	private PropertyDict steadyWorkWarnLevel;
	/*** 涉稳预警级别 日期 */
	private Date steadyWorkWarnLevelDate;
	/*** 备注 */
	private String otherRemark;

	private List<LedgerSteadyWorkPeopleInfo> ledgerSteadyWorkPeopleInfos;
	private boolean isAnonymity;

	public Integer getInvolvingSteadyNum() {
		return involvingSteadyNum;
	}

	public void setInvolvingSteadyNum(Integer involvingSteadyNum) {
		this.involvingSteadyNum = involvingSteadyNum;
	}

	public PropertyDict getInvolvingSteadyType() {
		return involvingSteadyType;
	}

	public void setInvolvingSteadyType(PropertyDict involvingSteadyType) {
		this.involvingSteadyType = involvingSteadyType;
	}

	public String getInvolvingSteadyInfo() {
		return involvingSteadyInfo;
	}

	public void setInvolvingSteadyInfo(String involvingSteadyInfo) {
		this.involvingSteadyInfo = involvingSteadyInfo;
	}

	public String getSteadyUnit() {
		return steadyUnit;
	}

	public void setSteadyUnit(String steadyUnit) {
		this.steadyUnit = steadyUnit;
	}

	public String getSteadyUser() {
		return steadyUser;
	}

	public void setSteadyUser(String steadyUser) {
		this.steadyUser = steadyUser;
	}

	public String getResolveUnit() {
		return resolveUnit;
	}

	public void setResolveUnit(String resolveUnit) {
		this.resolveUnit = resolveUnit;
	}

	public String getResolveUser() {
		return resolveUser;
	}

	public void setResolveUser(String resolveUser) {
		this.resolveUser = resolveUser;
	}

	public PropertyDict getAspirationType() {
		return aspirationType;
	}

	public void setAspirationType(PropertyDict aspirationType) {
		this.aspirationType = aspirationType;
	}

	public String getSteadyInfo() {
		return steadyInfo;
	}

	public void setSteadyInfo(String steadyInfo) {
		this.steadyInfo = steadyInfo;
	}

	public Integer getAttentionLevel() {
		return attentionLevel;
	}

	public void setAttentionLevel(Integer attentionLevel) {
		this.attentionLevel = attentionLevel;
	}

	public PropertyDict getSteadyWorkType() {
		return steadyWorkType;
	}

	public void setSteadyWorkType(PropertyDict steadyWorkType) {
		this.steadyWorkType = steadyWorkType;
	}

	public PropertyDict getSteadyWorkProblemType() {
		return steadyWorkProblemType;
	}

	public void setSteadyWorkProblemType(PropertyDict steadyWorkProblemType) {
		this.steadyWorkProblemType = steadyWorkProblemType;
	}

	public List<LedgerSteadyWorkPeopleInfo> getLedgerSteadyWorkPeopleInfos() {
		return ledgerSteadyWorkPeopleInfos;
	}

	public void setLedgerSteadyWorkPeopleInfos(
			List<LedgerSteadyWorkPeopleInfo> ledgerSteadyWorkPeopleInfos) {
		this.ledgerSteadyWorkPeopleInfos = ledgerSteadyWorkPeopleInfos;
	}

	public PropertyDict getSteadyWorkWarnLevel() {
		return steadyWorkWarnLevel;
	}

	public void setSteadyWorkWarnLevel(PropertyDict steadyWorkWarnLevel) {
		this.steadyWorkWarnLevel = steadyWorkWarnLevel;
	}

	@JSON(format = "yyyy-MM-dd")
	public Date getSteadyWorkWarnLevelDate() {
		return steadyWorkWarnLevelDate;
	}

	public void setSteadyWorkWarnLevelDate(Date steadyWorkWarnLevelDate) {
		this.steadyWorkWarnLevelDate = steadyWorkWarnLevelDate;
	}

	public boolean isAnonymity() {
		return isAnonymity;
	}

	public void setAnonymity(boolean isAnonymity) {
		this.isAnonymity = isAnonymity;
	}

	public String getOtherRemark() {
		return otherRemark;
	}

	public void setOtherRemark(String otherRemark) {
		this.otherRemark = otherRemark;
	}

}
