package com.tianque.domain;

import com.tianque.core.base.BaseDomain;

/**
 * 流动人口---未满16周岁随行人员
 */
@SuppressWarnings("serial")
public class Entourage extends BaseDomain {

	/** 流动人口id **/
	private Long trampResidentId;
	/** 姓名 */
	private String name;
	/** 关系 **/
	private String relation;
	/** 身份证号 **/
	private String idCardNo;
	/** 性别 */
	private PropertyDict gender;
	/** 有无预防接种证 */
	private boolean vaccinationCertificate;
	/** 是否在居住地上学 **/
	private boolean placeToSchool;

	public Long getTrampResidentId() {
		return trampResidentId;
	}

	public void setTrampResidentId(Long trampResidentId) {
		this.trampResidentId = trampResidentId;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getRelation() {
		return relation;
	}

	public void setRelation(String relation) {
		this.relation = relation;
	}

	public String getIdCardNo() {
		return idCardNo;
	}

	public void setIdCardNo(String idCardNo) {
		this.idCardNo = idCardNo;
	}

	public PropertyDict getGender() {
		return gender;
	}

	public void setGender(PropertyDict gender) {
		this.gender = gender;
	}

	public boolean isVaccinationCertificate() {
		return vaccinationCertificate;
	}

	public void setVaccinationCertificate(boolean vaccinationCertificate) {
		this.vaccinationCertificate = vaccinationCertificate;
	}

	public boolean isPlaceToSchool() {
		return placeToSchool;
	}

	public void setPlaceToSchool(boolean placeToSchool) {
		this.placeToSchool = placeToSchool;
	}

}
