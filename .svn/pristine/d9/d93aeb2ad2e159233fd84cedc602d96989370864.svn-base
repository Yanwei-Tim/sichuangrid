.orgDiv{position:absolute;width:150px;position:relative;z-index:99999;height: 150px;}
.orgDiv li{cursor:pointer;font:normal 12px/1em 'microsoft yahei';position:absolute;left:0px;color:#fff;text-align:center;}
.orgDiv li a{float:left;height:15px;display:inline-block;*display:inline;*zoom:1;line-height:15px;padding-left:20px;background:url(../images/map_org_bg.png) no-repeat left 0;color:#fff;}
.orgDiv li a span{float:left;height:15px;display:inline-block;*display:inline;*zoom:1;padding-right:15px;background:url(../images/map_org_bg.png) no-repeat right 0;}
.olLayerDiv div{margin-top:-5px;}









