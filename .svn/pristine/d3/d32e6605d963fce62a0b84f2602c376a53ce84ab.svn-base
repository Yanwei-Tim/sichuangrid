package com.tianque.baseInfo.companyPlace.domain.vo;

import com.tianque.baseInfo.companyPlace.domain.CompanyPlace;

public class CompanyPlaceVo extends CompanyPlace {

	private String sortField;
	private String order;

	private Long conditionsValue;

	public String getSortField() {
		return sortField;
	}

	public void setSortField(String sortField) {
		this.sortField = sortField;
	}

	public String getOrder() {
		return order;
	}

	public void setOrder(String order) {
		this.order = order;
	}

	public Long getConditionsValue() {
		return conditionsValue;
	}

	public void setConditionsValue(Long conditionsValue) {
		this.conditionsValue = conditionsValue;
	}
}
